{
    "name": "staudenmeir/eloquent-has-many-deep",
    "description": "Laravel Eloquent HasManyThrough relationship with unlimited levels",
    "license": "MIT",
    "authors": [
        {
            "name": "Jonas Staudenmeir",
            "email": "mail@jonas-staudenmeir.de"
        },
        {
            "name": "Hassan Raza Pasha",
            "email": "e.hassanrazapasha@gmail.com"
        }
    ],
     "repositories": [
        {
            "type": "vcs",
            "url": "https://github.com/paxha/has-many-through-deep"
        }
    ],
    "require": {
        "php": "^7.2",
        "illuminate/database": "^6.0"
    },
    "require-dev": {
        "illuminate/pagination": "^6.0",
        "laravel/homestead": "^9.0",
        "phpunit/phpunit": "^8.0",
        "paxha/eloquent-eager-limit": "^1.4"
    },
    "autoload": {
        "psr-4": {
            "Paxha\\HasManyThroughDeep\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Tests\\": "tests/"
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
